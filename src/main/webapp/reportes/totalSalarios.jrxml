	<queryString>
		<![CDATA[select x.*
, case when X.total_ganado < 5500 then 'Nivel Salarial<1,200.00 Bs. A 4,499.00 Bs.>' when TO_NUMBER(X.total_ganado) between 5500 and  12500 then 'Nivel Salarial<5,500.00 Bs. a 12,000.00 Bs.>' when TO_NUMBER(X.total_ganado) > 12500 then 'Nivel Salarial<12,00.00 Bs. a 18,000.00 Bs.>' else 'G4' end GRUPO_NIVEL_SALARIAL
, case when EDAD < 18 then '<Menor a 18>' when EDAD between 18 and  25 then '<18 a 25>' when EDAD between 26 and  45 then '<26 a 45>' when EDAD between 46 and  60 then '<46 a 60>' when EDAD > 60 then '<Mayor a 60>' else 'GE6' end  grupo_edad
from (
select nombre,clasificacion_laboral,nacionalidad,sexo,total_ganado,
trunc(months_between(sysdate, to_date(fecha_nacimiento,'dd/mm/yyyy'))/12) "EDAD"
 from doc_planilla_detalle) x
order by clasificacion_laboral, nacionalidad,total_ganado,  sexo, EDAD asc]]>
	</queryString>
